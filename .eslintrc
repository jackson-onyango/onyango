{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    },
  "plugins": ["import", "react"],
  "rules": {
    "no-console": 2,
    "radix": 0,
    "consistent-return": 0,
    "arrow-body-style": 1,
    "react/jsx-filename-extension": 0,
    "react/destructuring-assignment": 2,
    "react/prop-types": 2,
    "object-curly-newline": 2,
    "react/forbid-prop-types": 1,
    }
}
